{
  "name": "nextjsdev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "test": "jest --watchAll",
    "test:unit": "jest",
    "test:e2e-ci": "",
    "ts-node": "ts-node --log-error --pretty --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "build:seed:singleDir": "tsc ./prisma/seed.ts --outDir ./src/prisma/seedPipeline --types  --lib 'es2019'",
    "build:seed": "tsc ./prisma/seed.ts --types --lib 'es2019'",
    "test:coverage": "jest --coverage && open ./coverage/lcov-report/index.html",
    "createDevDatabase": "docker-compose up -d",
    "analyze-bundle": "ANALYZE=true next build",
    "db:studio": "prisma studio --schema ./src/prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema ./src/prisma/schema.prisma",
    "db:reseed": "prisma migrate reset --preview-feature --schema ./src/prisma/schema.prisma",
    "db:seed": "prisma db seed --preview-feature --schema ./src/prisma/schema.prisma",
    "jwt-proxy": "jwt-proxy 9000 3000 --silent --jwt user.jwt",
    "db:reset": "prisma db push --schema ./src/prisma/schema.prisma --force-reset && npm run db:seed",
    "db:generate": "prisma generate --schema ./src/prisma/schema.prisma",
    "dcheck": "mkdir -p .dependency-check && dependency-check --project .next --scan package-lock.json --format 'ALL' --out .dependency-check && open ./.dependency-check/dependency-check-report.html"
  },
  "dependencies": {
    "@babel/preset-react": "^7.14.5",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "@next/bundle-analyzer": "^11.0.0",
    "@prisma/client": "^2.25.0",
    "@tron/nextjs-auth-p1": "^1.2.2",
    "@types/crypto-js": "^4.0.1",
    "@types/faker": "^5.5.6",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.11",
    "@types/styled-components": "^5.1.10",
    "accesscontrol": "^2.2.1",
    "axios": "^0.21.1",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-styled-components": "^1.12.0",
    "babel-plugin-superjson-next": "^0.3.0",
    "crypto-js": "^4.0.0",
    "dayjs": "^1.10.5",
    "faker": "^5.5.3",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "next": "11.0.0",
    "node-fetch": "^2.6.1",
    "notistack": "^1.0.9",
    "postcss": "^8.3.4",
    "prisma": "^2.25.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "^3.17.0",
    "styled-components": "^5.3.0",
    "superjson": "^1.7.4",
    "tailwindcss": "^2.1.4",
    "ts-node": "^10.0.0",
    "twin.macro": "^2.5.0",
    "typescript": "^4.3.2",
    "unfetch": "^4.2.0",
    "whatwg-fetch": "^3.6.2",
    "zustand": "^3.5.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.5",
    "@testing-library/dom": "^7.31.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^7.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/node": "^15.12.2",
    "@types/node-fetch": "^2.5.10",
    "@types/test-listen": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "babel-jest": "^27.0.2",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "eslint": "^7.28.0",
    "eslint-config-next": "^11.0.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "isomorphic-unfetch": "^3.1.0",
    "jest": "^27.0.4",
    "mockdate": "^3.0.5",
    "msw": "0.27.0",
    "node-mocks-http": "^1.10.1",
    "prettier": "2.3.1",
    "pretty-quick": "^3.1.0",
    "query-string": "^7.0.0",
    "react-is": "^17.0.2",
    "setimmediate": "^1.0.5",
    "test-listen": "^1.1.0",
    "whatwg-fetch": "^3.6.2"
  },
  "babelMacros": {
    "twin": {
      "preset": "styled-components",
      "config": "./tailwind.config.js"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
