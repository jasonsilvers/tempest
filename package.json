{
  "name": "nextjsdev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "lint:next": "next lint",
    "start": "next start",
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "test": "jest --watchAll",
    "test:unit": "jest",
    "cypress:open": "cypress open --config-file cypress/config/development.json",
    "cypress:run": "cypress run --config-file cypress/config/development.json",
    "test:e2e-ci": "cypress run --config-file cypress/config/pipeline.json",
    "ts-node": "ts-node --log-error --pretty --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "build:seed:singleDir": "tsc ./src/prisma/seed.ts ./src/prisma/global.d.ts --outDir ./src/prisma/seedPipeline --allowSyntheticDefaultImports --lib 'es2019'",
    "build:seed": "tsc ./prisma/seed.ts ./prisma/global.d.ts --types --lib 'es2019'",
    "test:coverage": "jest --coverage && open ./coverage/lcov-report/index.html",
    "createDevDatabase": "docker-compose -f docker-compose-devdb.yml up -d ",
    "fullApp": "docker-compose -f docker-compose-fullapp.yml up -d ",
    "analyze-bundle": "ANALYZE=true next build",
    "db:studio": "prisma studio --schema ./src/prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema ./src/prisma/schema.prisma",
    "db:reseed": "prisma migrate reset --preview-feature --schema ./src/prisma/schema.prisma",
    "db:seed": "prisma db seed --preview-feature --schema ./src/prisma/schema.prisma",
    "jwt-proxy": "jwt-proxy 9000 3000 --silent --jwt user.jwt",
    "db:reset": "prisma db push --schema ./src/prisma/schema.prisma --force-reset && npm run db:seed",
    "db:generate": "prisma generate --schema ./src/prisma/schema.prisma",
    "dcheck": "mkdir -p .dependency-check && dependency-check --project .next --scan package-lock.json --format 'ALL' --out .dependency-check && open ./.dependency-check/dependency-check-report.html"
  },
  "dependencies": {
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@babel/preset-react": "^7.14.5",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@material-ui/pickers": "^3.3.10",
    "@next/bundle-analyzer": "^11.1.0",
    "@prisma/client": "^2.29.1",
    "@snekcode/mui-search-bar": "1.0.0",
    "@tron/nextjs-auth-p1": "^1.2.2",
    "@types/crypto-js": "^4.0.2",
    "@types/faker": "^5.5.8",
    "@types/jest": "^27.0.1",
    "@types/react": "^17.0.18",
    "@types/styled-components": "^5.1.12",
    "accesscontrol": "^2.2.1",
    "axios": "^0.21.4",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-styled-components": "^1.13.2",
    "babel-plugin-superjson-next": "^0.3.1",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.10.6",
    "faker": "^5.5.3",
    "file-to-array-buffer": "^1.0.0",
    "formidable": "^1.2.2",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "loglevel": "^1.7.1",
    "next": "11.1.0",
    "notistack": "^1.0.10",
    "postcss": "^8.3.6",
    "prisma": "^2.29.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "^3.19.6",
    "styled-components": "^5.3.0",
    "superjson": "^1.7.5",
    "tailwindcss": "^2.2.7",
    "ts-node": "^10.2.1",
    "twin.macro": "^2.6.2",
    "typescript": "^4.3.5",
    "xlsx": "^0.17.1",
    "zustand": "^3.5.9"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.0",
    "@testing-library/cypress": "^8.0.0",
    "@testing-library/dom": "^8.1.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.1",
    "@testing-library/user-event": "^13.2.1",
    "@types/node": "^16.6.2",
    "@types/node-fetch": "^2.5.12",
    "@types/test-listen": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "babel-jest": "^27.0.6",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "cypress": "^8.3.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.1",
    "identity-obj-proxy": "^3.0.0",
    "isomorphic-unfetch": "^3.1.0",
    "jest": "^27.0.6",
    "mockdate": "^3.0.5",
    "msw": "^0.27.0",
    "node-mocks-http": "^1.10.1",
    "prettier": "2.3.2",
    "pretty-quick": "^3.1.1",
    "query-string": "^7.0.1",
    "react-is": "^17.0.2",
    "setimmediate": "^1.0.5",
    "test-listen": "^1.1.0",
    "whatwg-fetch": "^3.6.2"
  },
  "babelMacros": {
    "twin": {
      "preset": "styled-components",
      "config": "./tailwind.config.js"
    }
  }
}
