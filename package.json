{
  "name": "cascade",
  "version": "1.3.0",
  "private": true,
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} src/prisma/seed.ts"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "lint:next": "next lint",
    "start": "next start",
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "test": "jest --watchAll --maxWorkers=50%",
    "test:unit": "prisma generate --schema ./src/prisma/schema.prisma && jest --max_old_space_size=8192",
    "pre-commit": "pretty-quick --staged",
    "cypress:open": "cypress open --config-file=cypress/config/development.config.ts",
    "cypress:run": "npm run db:reset && cypress run --config-file=cypress/config/development.config.ts",
    "test:e2e-ci": "cypress run --config-file=cypress/config/pipeline.config.ts",
    "build:seed:singleDir": "tsc ./src/prisma/seed.ts --outDir ./src/prisma/seedPipeline --allowSyntheticDefaultImports --lib 'es2019'",
    "build:seed": "tsc ./prisma/seed.ts --types --lib 'es2019'",
    "test:coverage": "jest --coverage && open ./coverage/lcov-report/index.html",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "createDevDatabase": "docker-compose -f docker-compose-devdb.yml up -d ",
    "fullApp": "docker-compose -f docker-compose-fullapp.yml up -d ",
    "analyze-bundle": "ANALYZE=true next build",
    "db:studio": "prisma studio --schema ./src/prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema ./src/prisma/schema.prisma",
    "db:push": "prisma db push --schema ./src/prisma/schema.prisma",
    "db:reset": "prisma migrate reset --schema ./src/prisma/schema.prisma --force",
    "db:seed": "prisma db seed --preview-feature --schema ./src/prisma/schema.prisma",
    "db:generate": "prisma generate --schema ./src/prisma/schema.prisma",
    "dcheck": "mkdir -p .dependency-check && dependency-check --project .next --scan package-lock.json --format 'ALL' --out .dependency-check && open ./.dependency-check/dependency-check-report.html",
    "prepare": "husky install",
    "force-resolutions": "npx npm-force-resolutions"
  },
  "resolutions": {
    "dset": "3.1.2",
    "moment": "2.29.4",
    "glob-parent": "6.0.2",
    "qs": "6.11.0"
  },
  "dependencies": {
    "@babel/plugin-transform-react-jsx": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@hookform/resolvers": "^2.9.5",
    "@mui/icons-material": "^5.8.4",
    "@mui/lab": "^5.0.0-alpha.89",
    "@mui/material": "^5.8.7",
    "@mui/styled-engine": "^5.8.7",
    "@mui/styled-engine-sc": "^5.3.0",
    "@mui/styles": "^5.8.7",
    "@mui/x-data-grid": "^5.13.0",
    "@mui/x-date-pickers": "^5.0.9",
    "@next/bundle-analyzer": "^12.0.8",
    "@prisma/client": "^3.15.2",
    "@tron/nextjs-auth-p1": "^1.4.0",
    "@types/crypto-js": "^4.1.0",
    "@types/jest": "^28.1.4",
    "@types/node": "^17.0.10",
    "@types/react": "^18.0.15",
    "@types/styled-components": "^5.1.21",
    "accesscontrol": "^2.2.1",
    "axios": "^0.27.2",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-styled-components": "^2.0.2",
    "babel-plugin-superjson-next": "^0.4.2",
    "casual": "^1.6.2",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.10.7",
    "file-to-array-buffer": "^1.0.0",
    "formidable": "^2.0.1",
    "fuse.js": "^6.5.3",
    "jest-environment-jsdom": "^28.1.2",
    "joi": "^17.5.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "lodash.debounce": "^4.0.8",
    "loglevel": "^1.8.0",
    "next": "^12.2.2",
    "node-fetch": "^2.6.7",
    "notistack": "^2.0.3",
    "postcss": "^8.4.5",
    "prisma": "^3.15.2",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.33.1",
    "react-query": "^3.34.12",
    "react-to-print": "^2.14.7",
    "styled-components": "^5.3.3",
    "superjson": "^1.8.0",
    "tailwindcss": "^3.0.16",
    "ts-node": "^10.4.0",
    "twin.macro": "^2.8.2",
    "typescript": "^4.5.5",
    "victory": "^36.6.0",
    "xlsx": "^0.17.5",
    "zustand": "^3.6.9"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.6",
    "@testing-library/cypress": "^8.0.7",
    "@testing-library/dom": "^8.16.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^13.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.2.1",
    "@types/test-listen": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "babel-jest": "^28.1.2",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "cypress": "^11.2.0",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "^12.0.8",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "fetch-retry": "^5.0.2",
    "husky": "^7.0.4",
    "i": "^0.3.7",
    "identity-obj-proxy": "^3.0.0",
    "isomorphic-unfetch": "^3.1.0",
    "jest": "^28.1.2",
    "mockdate": "^3.0.5",
    "msw": "^0.27.0",
    "next-router-mock": "^0.6.5",
    "node-mocks-http": "^1.11.0",
    "prettier": "2.5.1",
    "pretty-quick": "^3.1.3",
    "query-string": "^7.1.0",
    "react-is": "^18.2.0",
    "setimmediate": "^1.0.5",
    "test-listen": "^1.1.0",
    "whatwg-fetch": "^3.6.2"
  },
  "babelMacros": {
    "twin": {
      "preset": "styled-components",
      "config": "./tailwind.config.js"
    }
  }
}
