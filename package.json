{
  "name": "nextjsdev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "test": "jest --watchAll",
    "test:unit": "jest",
    "test:e2e-ci": "",
    "test:coverage": "jest --coverage",
    "createDevDatabase": "docker-compose up -d",
    "db:studio": "prisma studio",
    "db:migrate": "prisma migrate dev",
    "db:reseed": "prisma migrate reset --preview-feature",
    "db:seed": "prisma db seed --preview-feature",
    "jwt-proxy": "jwt-proxy 9000 3000 --silent --jwt user.jwt",
    "db:reset": "prisma db push --force-reset --preview-feature && npm run db:seed",
    "db:generate": "prisma generate"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@prisma/client": "^2.21.1",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.3",
    "@types/jest": "^26.0.20",
    "@types/styled-components": "^5.1.9",
    "@svgr/webpack": "^5.5.0",
    "@tron/nextjs-auth-p1": "^1.1.0",
    "accesscontrol": "^2.2.1",
    "axios": "^0.21.1",
    "babel-plugin-macros": "^3.0.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-styled-components": "^1.12.0",
    "babel-plugin-superjson-next": "^0.2.3",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "next": "10.0.5",
    "postcss": "^8.2.8",
    "prisma": "^2.20.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-error-boundary": "^3.1.1",
    "react-query": "^3.13.5",
    "styled-components": "^5.2.1",
    "superjson": "^1.7.2",
    "typescript": "^4.2.2",
    "tailwindcss": "^2.0.4",
    "twin.macro": "^2.3.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^7.29.4",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/react-hooks": "^5.1.0",
    "@testing-library/user-event": "^12.6.3",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.22.0",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-dom": "^4.0.0",
    "msw": "^0.27.2",
    "node-mocks-http": "^1.10.1",
    "prettier": "2.2.1",
    "pretty-quick": "^3.1.0",
    "react-is": "^17.0.1",
    "ts-node": "^9.1.1"
  },
  "babelMacros": {
    "twin": {
      "preset": "styled-components",
      "config": "./tailwind.config.js"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
